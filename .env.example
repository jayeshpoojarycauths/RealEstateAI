
# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000
VITE_API_RATE_LIMIT=100
VITE_API_RATE_WINDOW=60000

# Authentication
VITE_AUTH_TOKEN_KEY=auth_token
VITE_AUTH_REFRESH_TOKEN_KEY=auth_refresh_token
VITE_AUTH_USER_KEY=auth_user

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AUDIT_LOGS=true
VITE_ENABLE_MULTI_TENANCY=true
VITE_ENABLE_MFA=false

# Third-party Services
VITE_SENTRY_DSN=__SENTRY_DSN__
VITE_GOOGLE_MAPS_API_KEY=__GOOGLE_MAPS_API_KEY__
VITE_STRIPE_PUBLIC_KEY=__STRIPE_PUBLIC_KEY__

# Application Settings
VITE_APP_NAME=Real Estate AI
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development
VITE_APP_DEBUG=true

# File Upload
VITE_MAX_FILE_SIZE=10485760 # 10MB in bytes
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# Email Configuration
VITE_EMAIL_PROVIDER=smtp
VITE_SMTP_HOST=smtp.example.com
VITE_SMTP_PORT=587
VITE_SMTP_USERNAME=__SMTP_USERNAME__
VITE_SMTP_PASSWORD=__SMTP_PASSWORD__

# Analytics
VITE_ANALYTICS_ID=__ANALYTICS_ID__
VITE_ANALYTICS_ENABLED=true

# Internationalization
VITE_DEFAULT_LANGUAGE=en
VITE_AVAILABLE_LANGUAGES=en,es,fr

# Security
VITE_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
VITE_DEV_MODE=false
VITE_ENABLE_MOCK_DATA=false
